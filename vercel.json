{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/s/(.*)",
      "dest": "frontend/$1"
    },
    {
      "src": "/robots.txt",
      "dest": "frontend/robots.txt"
    },
    {
      "src": "/(.*)",
      "dest": "backend/main.py"
    }
  ]
}
